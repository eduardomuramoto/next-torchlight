{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "es2017",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@images/*": [
        "./public/images/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/get-support/courses/[slug]/page.tsx.backup",
    "src/components/Buttons/outlineButton.tsx.bckp",
    "src/app/api/checkout/route.ts.bckup",
    "src/app/get-support/courses/booking/[slug]/page.tsx.stripe",
    "src/app/get-involved/donate/success/page.tsx.bckp",
    "src/app/get-involved/donate/success/page.tsx.bckup"
  ],
  "exclude": [
    "node_modules"
  ]
}